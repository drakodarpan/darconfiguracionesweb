<div class="container content">
  <div class="row">
    <h3>Users</h3>
    <div class="columns">
      <table class="table-format">
        <tbody>
          <td class="title-center-table">ACCOUNT NUMBER</td>
          <td class="title-center-table">NOMBRE</td>
          <% @user.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>